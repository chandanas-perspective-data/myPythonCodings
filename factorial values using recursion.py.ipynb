   "source": [
    "# A Python program to calculate factorial values using recursion\n",
    "def factorial(n):\n",
    "    if n==0:\n",
    "        result=1\n",
    "    else:\n",
    "        result=n*factorial(n-1)\n",
    "    return result\n",
    "for i in range(1,11):\n",
    "    print(\"Factorial of {} is {}\".format(i,factorial(i)))\n",
    "            "
   ]
